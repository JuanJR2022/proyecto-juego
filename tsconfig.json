{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react-jsx", // o "react" si es para versiones anteriores de React
   "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["jest","node"]
    
  },
  "include": ["src/**/*", "tsconfig.json", "jest.config.js"],
  "exclude": ["node_modules"]
}
